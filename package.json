{
  "name": "react-scratch-universal",
  "version": "1.0.0",
  "description": "minimal setup for react, webpack, babel",
  "main": "dist/server.js",
  "scripts": {
    "build": "webpack",
    "build:server": "webpack -p --config webpack.config.server",
    "build:client": "webpack -p --config webpack.config",
    "build:all": "npm run clean && npm run build:server && npm run build:client",
    "clean": "rm -rf dist",
    "dev:client": "webpack-dev-server && node dist/app.bundle.js",
    "start": "node dist/server-build.js",
    "prestart": "npm run build:all",
    "dev": "webpack-dev-server"
  },
  "author": "tirta nugraha",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "webpack": "^3.7.1",
    "webpack-dev-server": "^2.9.2",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "express": "^4.16.2",
    "lodash": "^4.17.4",
    "node-fetch": "^1.7.3",
    "prop-types": "^15.6.0",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "source-map-support": "^0.5.0"
  }
}
